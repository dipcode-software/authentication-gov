{
    "name": "dipcode/authentication-gov",
    "description": "A PHP API that implements autenticacao.gov.pt service.",
    "keywords": ["AMA", "Authentication", "government", "oAuth2"],
    "license": "MIT",
    "authors": [
        {
          "name": "Dipcode",
          "email": "team@dipcode.com",
          "homepage": "https://dipcode.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Dipcode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dipcode\\Unit\\": "tests/"
        }
    },
    "scripts": {
        "test": [
          "phpcs --standard=phpcs.xml",
          "phpunit -c phpunit.xml"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    }
}
